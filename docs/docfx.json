{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/toc.yml",
        ],
        "exclude": [
          "**/obj/**",
          "docs/**"
          
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "docs/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata" : {
      "_enableSearch": true,
      "_appTitle": "TTG",
      "_appFooter": "<span>Copyright 2018-2019 TheTechGuy<br>Generated by <strong>DocFX</strong></span>",
      "_enableNewTab": true,
      "_gitContribute": {
       "repo": "https://github.com/thetechguy86/docs",
       "branch": "master"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "custom_template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}