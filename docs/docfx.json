{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/toc.yml"
        ],
        "exclude": [
          "**/obj/**",
          "docs/**",
          "pdf/**"
          
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "docs/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata" : {
      "_enableSearch": true,
      "_appTitle": "TheTechGuy Docs",
      "_appFooter": "<span>Copyright 2018-2019 TheTechGuy<br>Generated by <strong>DocFX</strong></span>",
      "_enableNewTab": true,
      "_gitContribute": {
       "repo": "https://github.com/thetechguy86/docs",
       "branch": "master"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/ttg"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  },

  "pdf": {
    "content": [
      {
        "files": [
            "**.yml"
        ],
        "exclude": [
          "**/toc.yml",
          "**/toc.md",
          "_site/**"
        ]
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "toc.yml",
          "*.md",
          "pdf/*"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site_pdf/**",
          "**/toc.yml",
          "**/toc.md",
          "_site/**"
        ]
      },
      {
        "files": "pdf/toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "**/media/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site_pdf/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**",
          "_site_pdf/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site_pdf",
    "template": [
      "pdf.default",
      "templates/pdf"
    ]
  }
}